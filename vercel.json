{
    "version": 2,
    "builds": [
        {
            "src": "dist/src/index.js",
            "use": "@now/node-server",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/src/index.js"
        }
    ]
}